@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":ViewCommand" as ViewCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

activate ViewCommand

ViewCommand -> Model : getFilteredModuleList()
activate Model

Model --> ViewCommand : Tracked Module List

opt module not found

    ViewCommand -> Model : getFilteredArchivedModuleList()
    Model --> ViewCommand : Archived Module List
    deactivate Model

end

@enduml
